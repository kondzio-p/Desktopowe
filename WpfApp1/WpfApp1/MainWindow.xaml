<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="800">
    <Grid x:Name="wRegexy">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="733*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="labelKodpocztowy" Grid.Column="1" Content="Kod pocztowy" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBoxKodpocztowy" Grid.Column="1" HorizontalAlignment="Left" Margin="10,80,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130" Height="36"/>
        <Button x:Name="btnKodpocztowy" Grid.Column="1" Content="Kod pocztowy" HorizontalAlignment="Left" Margin="154,80,0,0" VerticalAlignment="Top" Height="36" Width="110" Click="btnKodpocztowy_Click"/>
        <TextBox x:Name="txtBoxEmail" Grid.Column="1" HorizontalAlignment="Left" Margin="10,147,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130" Height="26"/>
        <Button x:Name="btnEmail" Grid.Column="1" Content="Sprawź e-mail" HorizontalAlignment="Left" Margin="154,147,0,0" VerticalAlignment="Top" Height="26" Width="110" Click="btnEmail_Click" />
        <Label x:Name="labelEmail" Grid.Column="1" Content="E-mail" HorizontalAlignment="Left" Margin="10,121,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Numery_telefonów" Grid.Column="1" Content="Numery telefonów" HorizontalAlignment="Left" Margin="10,182,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBoxNumeryTel" Grid.Column="1" HorizontalAlignment="Left" Margin="10,208,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" Height="29"/>
        <Button x:Name="btnNumeryTel" Grid.Column="1" Content="Poszukaj numerów" HorizontalAlignment="Left" Margin="154,208,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.586,0.819" Height="29" Width="110" Click="btnNumeryTel_Click"/>
        <Label Content="Data (YYYY-MM-DD)" HorizontalAlignment="Left"
                Height="32" Margin="9,256,0,0" VerticalAlignment="Top" Width="201" Grid.Column="1"></Label>
        <TextBox x:Name="txtBoxDate" Text="" HorizontalAlignment="Left" Height="32" Margin="9,288,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="201" Grid.Column="1"></TextBox>
        <Button x:Name="btnDate" Content="Sformatuj datę" Height="32" Margin="242,288,310,0" VerticalAlignment="Top" Width="181" Grid.Column="1" Click="btnDate_Click"/>
        <TextBox x:Name="txtBoxZdanie" Text="" HorizontalAlignment="Left" Height="32" Margin="9,367,0,0" TextWrapping="Wrap"
            VerticalAlignment="Top" Width="201" Grid.Column="1"/>
        <Label Grid.Column="1" Content="Wpisz jakiś tekst" HorizontalAlignment="Left" Margin="9,336,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnUsuwanie" Grid.Column="1" Content="Usuń zbędne spacje" HorizontalAlignment="Left" Margin="242,368,0,0" VerticalAlignment="Top" Height="31" Width="180" Click="btnUsuwanie_Click"/>
        <TextBox x:Name="txtBoxZdanie2" Text="" HorizontalAlignment="Left" Height="32" Margin="10,448,0,0" TextWrapping="Wrap"
            VerticalAlignment="Top" Width="201" Grid.Column="1"/>
        <Label Grid.Column="1" Content="Wpisz zdanie" HorizontalAlignment="Left" Margin="10,417,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnPodmien" Grid.Column="1" Content="Podmień wyrazy" HorizontalAlignment="Left" Margin="243,449,0,0" VerticalAlignment="Top" Height="31" Width="180" Click="btnPodmien_Click"/>
        <TextBox x:Name="txtBoxZdanie3" Text="" HorizontalAlignment="Left" Height="32" Margin="10,517,0,0" TextWrapping="Wrap"
            VerticalAlignment="Top" Width="201" Grid.Column="1"/>
        <Label Grid.Column="1" Content="Wpisz zdanie" HorizontalAlignment="Left" Margin="10,486,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnWymien" Grid.Column="1" Content="Wymień" HorizontalAlignment="Left" Margin="243,518,0,0" VerticalAlignment="Top" Height="31" Width="180" Click="btnWymien_Click"/>
    </Grid>
</Window>
